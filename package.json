{
  "name": "gousto-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-materialize": "^2.3.3",
    "react-slick": "^0.23.1"
  },
  "devDependencies": {
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/sass/imports/ -o public/build/css/ --output-style compressed",
    "watch-css": "node-sass-chokidar src/sass/imports/ -o src/sass/imports/ --watch --recursive"
  }
}
